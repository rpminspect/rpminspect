#: src/rpminspect.c:49
#, c-format
msgid ""
"Compare package builds for policy compliance and consistency.\n"
"\n"
msgstr ""
"Сравните сборки пакетов на соответствие политикам и согласованность.\n"
"\n"

#: src/rpminspect.c:50
#, c-format
msgid "Usage: %s [OPTIONS] [before build] [after build]\n"
msgstr "Использование: %s [ПАРАМЕТРЫ] [перед сборкой] [после сборки]\n"

#: src/rpminspect.c:51
#, c-format
msgid "Options:\n"
msgstr "Параметры:\n"

#: src/rpminspect.c:52
#, c-format
msgid "  -c FILE, --config=FILE   Configuration file to use\n"
msgstr "  -c FILE, --config=FILE   Файл конфигурации для использования\n"

#: src/rpminspect.c:53 src/rpminspect.c:69
#, c-format
msgid "                             (default: %s)\n"
msgstr "                             (по умолчанию: %s)\n"

#: src/rpminspect.c:54
#, c-format
msgid "  -p NAME, --profile=NAME  Configuration profile to use\n"
msgstr "  -p NAME, --profile=NAME  Профиль конфигурации для использования\n"

#: src/rpminspect.c:55
#, c-format
msgid "  -T LIST, --tests=LIST    List of tests to run\n"
msgstr "  -T LIST, --tests=LIST    Список тестов для запуска\n"

#: src/rpminspect.c:56
#, c-format
msgid "                             (default: ALL)\n"
msgstr "                             (по умолчанию: ALL)\n"

#: src/rpminspect.c:57
#, c-format
msgid "  -E LIST, --exclude=LIST  List of tests to exclude\n"
msgstr "  -E LIST, --exclude=LIST  Список тестов для исключения\n"

#: src/rpminspect.c:58
#, c-format
msgid "                             (default: none)\n"
msgstr "                             (по умолчанию: none)\n"

#: src/rpminspect.c:59
#, c-format
msgid "  -a LIST, --arches=LIST   List of architectures to check\n"
msgstr "  -a LIST, --arches=LIST   Список архитектур для проверки\n"

#: src/rpminspect.c:60
#, c-format
msgid "  -r STR, --release=STR    Product release string\n"
msgstr "  -r STR, --release=STR    Строка релиза продукта\n"

#: src/rpminspect.c:61
#, c-format
msgid "  -o FILE, --output=FILE   Write results to FILE\n"
msgstr "  -o FILE, --output=FILE   Записать результаты в FILE\n"

#: src/rpminspect.c:62
#, c-format
msgid "                             (default: stdout)\n"
msgstr "                             (по умолчанию: stdout)\n"

#: src/rpminspect.c:63
#, c-format
msgid "  -F TYPE, --format=TYPE   Format output results as TYPE\n"
msgstr "  -F TYPE, --format=TYPE   Отформатировать выходной результат как TYPE\n"

#: src/rpminspect.c:64
#, c-format
msgid "                             (default: text)\n"
msgstr "                             (по умолчанию: text)\n"

#: src/rpminspect.c:65
#, c-format
msgid "  -t TAG, --threshold=TAG  Result threshold triggering exit\n"
msgstr "  -t TAG, --threshold=TAG  Результат срабатывания триггера выхода\n"

#: src/rpminspect.c:66
#, c-format
msgid "                           failure (default: VERIFY)\n"
msgstr "                           неудача (по умолчанию: VERIFY)\n"

#: src/rpminspect.c:67
#, c-format
msgid "  -l, --list               List available tests and formats\n"
msgstr "  -l, --list               Список доступных тестов и форматов\n"

#: src/rpminspect.c:68
#, c-format
msgid "  -w PATH, --workdir=PATH  Temporary directory to use\n"
msgstr "  -w PATH, --workdir=PATH  Временный каталог для использования\n"

#: src/rpminspect.c:70
#, c-format
msgid "  -f, --fetch-only         Fetch builds only, do not perform inspections\n"
msgstr "  -f, --fetch-only         Получать только сборки, не проверять\n"

#: src/rpminspect.c:71
#, c-format
msgid "                             (implies -k)\n"
msgstr "                             (подразумевает -k)\n"

#: src/rpminspect.c:72
#, c-format
msgid "  -k, --keep               Do not remove the comparison working files\n"
msgstr "  -k, --keep               Не удалять рабочие файлы сравнения\n"

#: src/rpminspect.c:73
#, c-format
msgid "  -d, --debug              Debugging mode output\n"
msgstr "  -d, --debug              Вывод в режиме отладки\n"

#: src/rpminspect.c:74
#, c-format
msgid "  -v, --verbose            Verbose inspection output\n"
msgstr "  -v, --verbose            Подробный вывод проверки\n"

#: src/rpminspect.c:75
#, c-format
msgid "                           when finished, display full path\n"
msgstr "                           по завершении отобразить полный путь\n"

#: src/rpminspect.c:76
#, c-format
msgid "  -?, --help               Display usage information\n"
msgstr "  -?, --help               Показать информацию об использовании\n"

#: src/rpminspect.c:77
#, c-format
msgid "  -V, --version            Display program version\n"
msgstr "  -V, --version            Показать версию программы\n"

#: src/rpminspect.c:78
#, c-format
msgid ""
"\n"
"See the rpminspect(1) man page for more information.\n"
msgstr ""
"\n"
"См. rpminspect(1) man страницу для дополнительной информации.\n"

